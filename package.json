{
  "name": "mk-simple-logger",
  "version": "0.1.3",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "Marck C. Guzman <marck@marck-devs.com> (http://marck-devs.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/Marck-Devs/node-simple-logger"
  },
  "devDependencies": {
    "tsc": "^2.0.3",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "chalk": "^4.1.2"
  },
  "scripts": {
    "clean": "rm -rf build compile dist",
    "compile": "npx tsc",
    "babel": "babel compile/src --out-dir build",
    "exec": "clear && node build/index.js",
    "build": "yarn compile && yarn babel",
    "start": "yarn build && yarn run exec",
    "prod": "yarn clean && yarn compile &&  yarn babel compile/src --out-dir dist "
  },
  "displayName": "simple-logger",
  "description": "Simple logger for node",
  "keywords": [
    "logger",
    "log",
    "logging",
    "node"
  ]
}